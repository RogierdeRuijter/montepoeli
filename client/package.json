{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --host 0.0.0.0",
    "start:dev-no-host": "ng serve --host 0.0.0.0 --disable-host-check",
    "start:dev-no-host-aot": "ng serve --host 0.0.0.0 --aot --disable-host-check",
    "build": "ng build --prod",
    "test": "ng test --watch=false",
    "test:jest": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e --no-webdriver-update",
    "e2e:build-server": "npm run webdriver-update && npm run webdriver-update:protractor && ng e2e --no-webdriver-update",
    "protractor": "protractor",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-storybook-web-app": "build-storybook -c .storybook -o .out",
    "install-start": "npm install && npm start",
    "update-angular": "ng update @angular/cli @angular/core @angular/material",
    "screenshot": "storybook-chrome-screenshot -p 9001 -c .storybook",
    "webdriver-update": "webdriver-manager clean && webdriver-manager update",
    "webdriver-update:protractor": "./node_modules/protractor/bin/webdriver-manager update"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.2",
    "@angular/cdk": "~8.0.0",
    "@angular/common": "~8.0.2",
    "@angular/compiler": "~8.0.2",
    "@angular/core": "~8.0.2",
    "@angular/forms": "~8.0.2",
    "@angular/material": "~8.0.0",
    "@angular/platform-browser": "~8.0.2",
    "@angular/platform-browser-dynamic": "~8.0.2",
    "@angular/router": "~8.0.2",
    "@fortawesome/angular-fontawesome": "0.4.0",
    "@fortawesome/fontawesome-svg-core": "1.2.18",
    "@fortawesome/free-solid-svg-icons": "5.8.2",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "bootstrap": "4.3.1",
    "bootstrap-material-design": "4.1.2",
    "core-js": "~3.1.3",
    "hammerjs": "^2.0.8",
    "intl": "~1.2.5",
    "ngx-cookie-service": "^2.2.0",
    "ngx-skeleton-loader": "^1.2.0",
    "ngx-toastr": "10.0.4",
    "rxjs": "~6.5.2",
    "webdriver-manager": "^12.1.6",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.2",
    "@angular/language-service": "~8.0.2",
    "@babel/core": "7.4.5",
    "@storybook/addon-actions": "5.0.11",
    "@storybook/addon-links": "5.0.11",
    "@storybook/addon-notes": "5.0.11",
    "@storybook/addon-storyshots": "5.0.11",
    "@storybook/addon-viewport": "5.0.11",
    "@storybook/addons": "5.0.11",
    "@storybook/angular": "5.0.11",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/jest": "^24.0.15",
    "@types/node": "~12.0.8",
    "babel-loader": "8.0.6",
    "codelyzer": "~5.0.1",
    "husky": "~2.3.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "mongodb": "latest",
    "phantomjs-prebuilt": "~2.1.16",
    "protractor": "^5.4.2",
    "storybook-chrome-screenshot": "1.4.0",
    "ts-node": "~8.2.0",
    "tslint": "~5.16.0",
    "typescript": "3.4.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./src/setupJest.ts"
    ]
  }
}
