{
  "name": "frontend",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --host 0.0.0.0",
    "start:dev-no-host": "ng serve --host 0.0.0.0",
    "start:dev-no-host-ssl": "ng serve frontend --ssl --host 0.0.0.0 --disable-host-check",
    "start:dev-no-host-aot": "ng serve --host 0.0.0.0 --aot --disable-host-check",
    "build": "ng build",
    "build:pre-push": "ng build --aot && rm -r dist",
    "build:prod": "ng build --configuration=production && gzipper --verbose ./dist",
    "build:e2e": "ng build --configuration=e2e",
    "build:local": "ng build --configuration=local",
    "install-node-modules": "npm install",
    "test": "npm run jest",
    "test:watch": "ng test",
    "test:cov": "npm run jest -- --coverage",
    "jest": "jest",
    "test:no-cache": "jest --no-cache",
    "lint": "ng lint frontend",
    "lint:fix": "ng lint frontend --fix",
    "e2e": "ng ../e2e --no-webdriver-update",
    "e2e:build-server": "npm run webdriver-update && npm run webdriver-update:protractor && ng ../e2e --no-webdriver-update",
    "protractor": "npm run webdriver-update && protractor",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-storybook-web-app": "build-storybook -c .storybook -o .out",
    "install-start": "npm install && npm start",
    "update-angular": "ng update @angular/cli @angular/core @angular/material",
    "update-angular-next": "ng update --next @angular/cli @angular/core @angular/material",
    "screenshot": "storybook-chrome-screenshot -p 9001 -c .storybook",
    "webdriver-update": "webdriver-manager clean && webdriver-manager update",
    "webdriver-update:protractor": "./node_modules/protractor/bin/webdriver-manager update",
    "compile-ivy-dependencies": "ngcc -p es2015, es2015 -l info --create-ivy-entry-points",
    "generate-component": "ng generate component -c OnPush --skip-tests",
    "compile-ivy-dependencies-test": "ngcc --properties browser module main --first-only --create-ivy-entry-points",
    "bundle-report": "webpack-bundle-analyzer dist/frontend/stats-es2015.json",
    "build-and-push-docker-node-modules": "npm run build-docker-base-node-modules && npm run push-docker-base-node-modules",
    "build-docker-base-node-modules": "docker build -f Dockerfile.node_modules -t montepoeli.club/montepoeli-client-base-node-modules:latest . --compress --pull",
    "push-docker-base-node-modules": "docker push montepoeli.club/montepoeli-client-base-node-modules:latest",
    "iPhone-simulator": "/Applications/Xcode.app/Contents/Developer/Applications/Simulator.app/Contents/MacOS/Simulator -CurrentDeviceID 8528838E-4B47-4F0E-B415-E87F8C8A6163"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "0.900.6",
    "@angular/animations": "9.0.6",
    "@angular/cdk": "9.1.2",
    "@angular/cli": "9.0.6",
    "@angular/common": "9.0.6",
    "@angular/compiler": "9.0.6",
    "@angular/compiler-cli": "9.0.6",
    "@angular/core": "9.0.6",
    "@angular/forms": "9.0.6",
    "@angular/material": "9.1.2",
    "@angular/platform-browser": "9.0.6",
    "@angular/platform-browser-dynamic": "9.0.6",
    "@angular/router": "9.0.6",
    "@angular/service-worker": "9.0.6",
    "@fortawesome/angular-fontawesome": "0.6.0",
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/node": "13.9.1",
    "bootstrap": "4.4.1",
    "bootstrap-material-design": "4.1.2",
    "core-js": "3.6.4",
    "gzipper": "3.5.0",
    "intl": "1.2.5",
    "ngx-cookie-service": "3.0.2",
    "ngx-toastr": "12.0.0",
    "rxjs": "6.5.4",
    "tslib": "1.11.1",
    "typescript": "3.7.5",
    "webdriver-manager": "12.1.7",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "codelyzer": "5.2.1",
    "@testing-library/angular": "8.2.0",
    "@testing-library/jest-dom": "5.1.1",
    "@types/jasmine": "3.5.9",
    "@types/jasminewd2": "2.0.8",
    "@types/jest": "25.1.4",
    "jest": "24.9.0",
    "jest-preset-angular": "7.1.1",
    "@types/webpack-env": "1.15.1",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "ts-node": "8.6.2",
    "tslint": "6.1.0",
    "@storybook/addon-actions": "5.3.17",
    "@storybook/addon-storyshots": "5.3.17",
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@storybook/addon-links": "5.3.17",
    "@storybook/addon-notes": "5.3.17",
    "@storybook/addon-viewport": "5.3.17",
    "@storybook/addons": "5.3.17",
    "@storybook/angular": "5.3.17"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./src/setupJest.ts"
    ]
  }
}
