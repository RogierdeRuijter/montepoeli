    "@angular/service-worker": "9.0.6",
    "@fortawesome/angular-fontawesome": "0.6.0",
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "@nestjs/common": "6.11.8",
    "@nestjs/core": "6.11.8",
    "@nestjs/jwt": "6.1.2",
    "@nestjs/mongoose": "6.3.1",
    "@nestjs/passport": "6.2.0",
    "@nestjs/platform-express": "6.11.8",
    "@nestjs/testing": "6.11.8",
    "@nestjs/typeorm": "6.3.3",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "@nrwl/angular": "9.1.2",
    "@nrwl/node": "9.2.2",
    "@types/bcrypt": "3.0.0",
    "@types/express": "4.17.2",
    "@types/faker": "4.1.10",
    "@types/mongoose": "^5.7.6",
    "@types/node": "13.9.1",
    "@types/supertest": "2.0.8",
    "bcrypt": "4.0.1",
    "bootstrap": "4.4.1",
    "bootstrap-material-design": "4.1.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "core-js": "3.6.4",
    "faker": "4.1.0",
    "gzipper": "3.5.0",
    "intl": "1.2.5",
    "mongoose": "5.9.4",
    "mysql": "2.18.1",
    "ngx-cookie-service": "3.0.2",
    "ngx-toastr": "12.0.0",
    "passport": "0.4.1",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.4",
    "tslib": "1.11.1",
    "typeorm": "0.2.24",
    "typescript": "3.7.5",
    "webdriver-manager": "12.1.7",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@nrwl/workspace": "9.1.2",
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.4",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@storybook/addon-actions": "5.3.17",
    "@storybook/addon-links": "5.3.17",
    "@storybook/addon-notes": "5.3.17",
    "@storybook/addon-storyshots": "5.3.17",
    "@storybook/addon-viewport": "5.3.17",
    "@storybook/addons": "5.3.17",
    "@storybook/angular": "5.3.17",
    "@testing-library/angular": "8.2.0",
    "@testing-library/jest-dom": "5.1.1",
    "@types/jasmine": "3.5.9",
    "@types/jasminewd2": "2.0.8",
    "@types/jest": "25.1.4",
    "@types/webpack-env": "1.15.1",
    "babel-loader": "8.0.6",
    "codelyzer": "5.2.1",
    "dotenv": "6.2.0",
    "husky": "4.2.3",
    "jasmine": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "25.3.0",
    "jest-preset-angular": "^8.1.3",
    "lerna": "3.20.2",
    "moment": "2.24.0",
    "mongodb": "3.5.5",
    "nodemon": "2.0.2",
    "path": "0.12.7",
    "prettier": "1.19.1",
    "protractor": "5.4.3",
    "semantic-release": "17.0.4",
    "supertest": "4.0.2",
    "ts-jest": "25.4.0",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.0",
    "webdriver-manager": "12.1.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run pre-commit",
      "pre-push": "lerna run --parallel test && lerna run --parallel build:pre-push",
      "post-merge": "yarn install-sub-modules"
    }
  }
}
